<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/sudokuback4"
    tools:context=".Sudoku">


    <GridView
        android:id="@+id/gridView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:gravity="center"
        android:horizontalSpacing="1dp"
        android:numColumns="9"
        android:stretchMode="columnWidth"
        android:verticalSpacing="274dp"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="241dp" />

    <TextView
        android:id="@+id/Time"
        android:layout_width="366dp"
        android:layout_height="161dp"
        android:text="60"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="96sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="326dp"
        tools:layout_editor_absoluteY="1362dp" />

    <TextView
        android:id="@+id/box"
        android:layout_width="800dp"
        android:layout_height="800dp"
        android:gravity="center"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.053"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/gridOne"
        android:layout_width="266dp"
        android:layout_height="266dp"
        android:gravity="center"
        android:text="1"
        android:textSize="10sp"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/gridFour"
        app:layout_constraintEnd_toStartOf="@+id/gridTwo"
        app:layout_constraintStart_toStartOf="@+id/box"
        app:layout_constraintTop_toTopOf="@+id/box" />

    <TextView
        android:id="@+id/gridTwo"
        android:layout_width="266dp"
        android:layout_height="266dp"
        android:gravity="center"
        android:text="2"
        android:textSize="10sp"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/gridFive"
        app:layout_constraintEnd_toStartOf="@+id/gridThree"
        app:layout_constraintStart_toEndOf="@+id/gridOne"
        app:layout_constraintTop_toTopOf="@+id/box" />

    <TextView
        android:id="@+id/gridThree"
        android:layout_width="266dp"
        android:layout_height="266dp"
        android:gravity="center"
        android:text="3"
        android:textSize="10sp"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/gridSix"
        app:layout_constraintEnd_toEndOf="@+id/box"
        app:layout_constraintStart_toEndOf="@+id/gridTwo"
        app:layout_constraintTop_toTopOf="@+id/box" />

    <TextView
        android:id="@+id/gridFour"
        android:layout_width="266dp"
        android:layout_height="266dp"
        android:gravity="center"
        android:text="4"
        android:textSize="10sp"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/gridSeven"
        app:layout_constraintEnd_toStartOf="@+id/gridFive"
        app:layout_constraintStart_toStartOf="@+id/box"
        app:layout_constraintTop_toBottomOf="@+id/gridOne"
        tools:layout_editor_absoluteY="297dp" />

    <TextView
        android:id="@+id/gridFive"
        android:layout_width="266dp"
        android:layout_height="266dp"
        android:gravity="center"
        android:text="5"
        android:textSize="10sp"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/gridEight"
        app:layout_constraintEnd_toStartOf="@+id/gridSix"
        app:layout_constraintStart_toEndOf="@+id/gridFour"
        app:layout_constraintTop_toBottomOf="@+id/gridTwo" />

    <TextView
        android:id="@+id/gridSix"
        android:layout_width="266dp"
        android:layout_height="266dp"
        android:layout_marginEnd="1dp"
        android:gravity="center"
        android:text="6"
        android:textSize="10sp"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/gridNine"
        app:layout_constraintEnd_toEndOf="@+id/box"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/gridFive"
        app:layout_constraintTop_toBottomOf="@+id/gridThree"
        app:layout_constraintVertical_bias="0.971" />

    <TextView
        android:id="@+id/gridSeven"
        android:layout_width="266dp"
        android:layout_height="266dp"
        android:gravity="center"
        android:text="7"
        android:textSize="10sp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/box"
        app:layout_constraintEnd_toStartOf="@id/gridEight"
        app:layout_constraintStart_toStartOf="@+id/box"
        app:layout_constraintTop_toBottomOf="@+id/gridFour"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/gridEight"
        android:layout_width="266dp"
        android:layout_height="266dp"
        android:gravity="center"
        android:text="8"
        android:textSize="10sp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/box"
        app:layout_constraintEnd_toStartOf="@+id/gridNine"
        app:layout_constraintStart_toEndOf="@+id/gridSeven"
        app:layout_constraintTop_toBottomOf="@id/gridFive" />

    <TextView
        android:id="@+id/gridNine"
        android:layout_width="266dp"
        android:layout_height="266dp"
        android:gravity="center"
        android:text="9"
        android:textSize="10sp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/box"
        app:layout_constraintEnd_toEndOf="@+id/box"
        app:layout_constraintStart_toEndOf="@+id/gridEight"
        app:layout_constraintTop_toBottomOf="@+id/gridSix" />

    <TextView
        android:id="@+id/scriptName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sudoku"
        android:textColor="@color/purple_700"
        android:textSize="50sp"
        app:layout_constraintBottom_toTopOf="@+id/box"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.326" />

    <ImageButton
        android:id="@+id/TrophyIcon"
        android:layout_width="171dp"
        android:layout_height="136dp"
        android:layout_marginStart="264dp"
        android:layout_marginEnd="485dp"
        android:layout_marginBottom="35dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/img_5" />

    <ImageButton
        android:id="@+id/profileIcon"
        android:layout_width="175dp"
        android:layout_height="136dp"
        android:layout_marginStart="709dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="36dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/img_8" />

    <ImageButton
        android:id="@+id/HomeButton"
        android:layout_width="173dp"
        android:layout_height="135dp"
        android:layout_marginStart="496dp"
        android:layout_marginEnd="251dp"
        android:layout_marginBottom="35dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/img_7" />

    <ImageButton
        android:id="@+id/settingsIcon"
        android:layout_width="171dp"
        android:layout_height="136dp"
        android:layout_marginStart="45dp"
        android:layout_marginEnd="704dp"
        android:layout_marginBottom="35dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/img_6" />

    <ImageButton
        android:id="@+id/delete"
        android:layout_width="81dp"
        android:layout_height="86dp"
        android:layout_marginStart="268dp"
        android:src="@drawable/baseline_delete_24"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.095" />

    <ImageButton
        android:id="@+id/undo"
        android:layout_width="83dp"
        android:layout_height="88dp"
        android:layout_marginStart="60dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.239"
        app:layout_constraintStart_toEndOf="@+id/delete"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.094"
        app:srcCompat="@drawable/baseline_undo_24" />


</androidx.constraintlayout.widget.ConstraintLayout>